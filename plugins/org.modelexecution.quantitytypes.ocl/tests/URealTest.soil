-- EXAMPLE OF USES OF UReal VALUES
!new UReal('a')
!new UReal('b')
-------------------------------------------
!a.x :=1.0
!a.u :=1.5
!b.x :=5.0
!b.u :=1.0
!b1:=a.equals(b)--should be false
?b1
!b2:=a.lt(b)--should be true
?b2
!b3:=a.gt(b)--should be false
?b3
!aEQb:=a.uEquals(b) 
?aEQb.c -- 0.1068
!aLTb:=a.uLt(b) 
?aLTb.c -- 0.8931
!aGTb:=a.uGt(b)
?aGTb.c -- 0.00001
