!b:=Km.isDimensionless()
?b -- should be false
!b:=s.isDimensionless()
?b -- should be false
!b:=rad.isDimensionless()
?b -- should be false
!b:=U.isDimensionless()
?b -- should be true
!b:=Kms.isDimensionless()
?b -- should be false
------------------------------------
!b:=Km.isUnitless()
?b -- should be false
!b:=s.isUnitless()
?b -- should be false
!b:=rad.isUnitless()
?b -- should be true
!b:=U.isUnitless()
?b -- should be true
!b:=Kms.isUnitless()
?b -- should be false
------------------------------------
!b:=Km.equals(Km)
?b -- should be true
!b:=Km.equals(Kms)
?b -- should be false
!b:=Km.equals(U)
?b -- should be false
------------------------------------
!b:=Km.isCompatibleWith(Km)
?b -- should be true
!b:=Km.isCompatibleWith(Kms)
?b -- should be false
!b:=Km.isCompatibleWith(U)
?b -- should be false
-----------------------------------
!b:=Km.isBaseUnit()
?b -- should be true
!b:=rad.isBaseUnit()
?b -- should be true
!b:=Kms.isBaseUnit()
?b -- should be false
!b:=U.isBaseUnit()
?b -- should be false
